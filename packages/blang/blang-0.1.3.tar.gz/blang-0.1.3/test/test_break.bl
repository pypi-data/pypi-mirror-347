# expected_exit_code = 0
# Break out of a loop should work

def main(): u8 {
    for 0..10 as a:u8 {
      if (a == 5) {
            break
        }
    }
    if (a != 5) {
        return 1
    }

    a=0
    while (a<10) {
       if (a>5) {
           break
       }
       a=a+1
    }
    if (a>6) {
        return 1
    }
    return 0
}
