{
    "namespace": "suzieq",
    "name": "sqPoller",
    "type": "counter",
    "fields": [
        {
            "name": "sqvers",
            "type": "string",
            "partition": 0,
            "default": "3.0",
            "suppress": true,
            "description": "Schema version, not selectable"
        },
        {
            "name": "gatherTime",
            "type": {
                "type": "array",
                "items": {
                    "type": "float",
                    "name": "time"
                }
            },
            "display": 4,
            "description": "[min, max, avg] of time(ms) taken to get data, computed over the greater between the last 5 mins and the polling period"
        },
        {
            "name": "totalTime",
            "type": {
                "type": "array",
                "items": {
                    "type": "float",
                    "name": "time"
                }
            },
            "display": 5,
            "description": "[min, max, avg] of time(ms) taken to get & process data,computed over the greater between the last 5 mins and the polling period"
        },
        {
            "name": "svcQsize",
            "type": {
                "type": "array",
                "items": {
                    "type": "float",
                    "name": "svcQsize"
                }
            },
            "display": 6,
            "description": "[min, max, avg] of service queue length, computed over the greater between the last 5 mins and the polling period"
        },
        {
            "name": "wrQsize",
            "type": {
                "type": "array",
                "items": {
                    "type": "float",
                    "name": "wrQsize"
                }
            },
            "display": 7,
            "description": "[min, max, avg] of write queue length, computed over the greater between the last 5 mins and the polling period"
        },
        {
            "name": "nodeQsize",
            "type": {
                "type": "array",
                "items": {
                    "type": "float",
                    "name": "nodeQsize"
                }
            },
            "display": 8,
            "description": "[min, max, avg] of node queue length, computed over the greater between the last 5 mins and the polling period"
        },
        {
            "name": "rxBytes",
            "type": {
                "type": "array",
                "items": {
                    "type": "double",
                    "name": "rxBytes"
                }
            },
            "display": 9,
            "description": "[min, max, avg] of bytes read, computed over the greater between the last 5 mins and the polling period"
        },
        {
            "name": "version",
            "type": "string",
            "description": "The suzieq version used to create this record"
        },
        {
            "name": "pollExcdPeriodCount",
            "type": "long",
            "display": 10,
            "description": "Number of times the poller has exceeded the poll period"
        },
        {
            "name": "status",
            "type": "long",
            "description": "Service polling status code. 0 or 200 means successfully polled, -1 means time timeout, other values mean failed to poll"
        },
        {
            "name": "statusStr",
            "type": "string",
            "display": 3,
            "description": "Service polling status string",
	    "depends": "status"
        },
        {
            "name": "nodesPolledCnt",
            "type": "long",
            "description": "Number of nodes polled"
        },
        {
            "name": "nodesFailedCnt",
            "type": "long",
            "description": "Number of nodes that could not be polled"
        },
        {
            "name": "service",
            "type": "string",
            "key": 2,
            "display": 2,
            "partition": 2,
            "description": "The polled service (a.k.a table) name"
        },
        {
            "name": "hostname",
            "type": "string",
            "key": 1,
            "display": 1,
            "partition": 2,
            "description": "Hostname associated with this record"
        },
        {
            "name": "namespace",
            "type": "string",
            "key": 0,
            "display": 0,
            "partition": 1,
            "description": "The namespace associated with this record"
        },
        {
            "name": "timestamp",
            "type": "long",
            "display": 11,
            "description": "The timestamp of the record"
        },
        {
            "name": "active",
            "type": "boolean",
            "description": "If this entry is active or deleted"
        }
    ],
    "recordType": "counters"
}
