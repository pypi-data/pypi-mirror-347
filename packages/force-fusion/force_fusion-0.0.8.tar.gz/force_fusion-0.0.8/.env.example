# Force Fusion Configuration
# This file can be used to override default settings in config.py

# Color for negative acceleration display (red)
FORCE_FUSION_ACCEL_COLOR_NEGATIVE="#e74c3c"

# Color for positive acceleration display (green)
FORCE_FUSION_ACCEL_COLOR_POSITIVE="#2ecc71"

# Maximum acceleration value in m/s² for displays
FORCE_FUSION_ACCEL_MAX=10

# Minimum acceleration value in m/s² for displays
FORCE_FUSION_ACCEL_MIN=-10

# Primary accent color for the UI (blue)
FORCE_FUSION_ACCENT_COLOR="#3498db"

# Update interval in milliseconds for attitude displays
FORCE_FUSION_ATTITUDE_UPDATE_INTERVAL=100

# Main application background color (dark gray)
FORCE_FUSION_BACKGROUND_COLOR="#1f1f1f"

# Border color for instrument bezels (darker gray)
FORCE_FUSION_BEZEL_BORDER_COLOR="#323232"

# Background color for instrument bezels (medium gray)
FORCE_FUSION_BEZEL_COLOR="#3c3c3c"

# Path for storing received vehicle data CSV file
FORCE_FUSION_CSV_PATH="data/vehicle_data.csv"

# Color used for dangerous or critical values (red)
FORCE_FUSION_DANGER_COLOR="#e74c3c"

# Enable for development and debugging mode
FORCE_FUSION_DEBUG_MODE=False

# Default map center coordinates (longitude,latitude) - Kennedy Space Center area
FORCE_FUSION_DEFAULT_CENTER=-81.04897348153887,29.18825368942673

# Default data source for vehicle data (simulated or websocket)
FORCE_FUSION_DEFAULT_DATA_SOURCE="websocket"

# Default zoom level for maps (1-19, higher means more zoomed in)
FORCE_FUSION_DEFAULT_ZOOM=17

# Default map style for detailed map view (satellite or street)
FORCE_FUSION_DETAILMAP_DEFAULT_STYLE="satellite"

# Maximum G force to display in GG diagram (lateral/longitudinal acceleration)
FORCE_FUSION_GG_DIAGRAM_MAX_G=1.5

# Minimum G force to display in GG diagram (lateral/longitudinal acceleration)
FORCE_FUSION_GG_DIAGRAM_MIN_G=-1.5

# Update interval in milliseconds for GG diagram
FORCE_FUSION_GG_DIAGRAM_UPDATE_INTERVAL=100

# GPS update interval in milliseconds
FORCE_FUSION_GPS_UPDATE_INTERVAL=1000

# Color for heading indicator (red)
FORCE_FUSION_HEADING_COLOR="#ff0000"

# Your Mapbox API token for map services
FORCE_FUSION_MAPBOX_TOKEN="YOUR_MAPBOX_TOKEN_HERE"

# Background color for map displays (dark gray)
FORCE_FUSION_MAP_BACKGROUND_COLOR="#282828"

# CSS style for active map style button
FORCE_FUSION_MAP_BUTTON_ACTIVE_STYLE="background-color: #3498db; padding: 8px 15px; border-radius: 4px; color: white; margin-right: 10px;"

# CSS style for inactive map style button
FORCE_FUSION_MAP_BUTTON_INACTIVE_STYLE="background-color: #34495e; padding: 8px 15px; border-radius: 4px; color: white; margin-right: 10px;"

# Grid line color for map overlays (light gray)
FORCE_FUSION_MAP_GRID_COLOR="#aaaaaa"

# Opacity value (0-255) for map grid overlay
FORCE_FUSION_MAP_GRID_OPACITY=100

# URLs for different map tile styles (OpenStreetMap for street, Bing Maps for satellite)
FORCE_FUSION_MAP_TILE_URLS={'street': 'http://a.tile.openstreetmap.org/{z}/{x}/{y}.png', 'satellite': 'http://ecn.t0.tiles.virtualearth.net/tiles/a{q}.jpeg?g=1'}

# Update interval in milliseconds for maps
FORCE_FUSION_MAP_UPDATE_INTERVAL=100

# Default map style for minimap (satellite or street)
FORCE_FUSION_MINIMAP_DEFAULT_STYLE="satellite"

# Maximum pitch angle in degrees
FORCE_FUSION_PITCH_MAX=40

# Minimum pitch angle in degrees
FORCE_FUSION_PITCH_MIN=-40

# Color for pitch indicator pointer (amber)
FORCE_FUSION_PITCH_POINTER_COLOR="#ffbf00"

# Maximum roll angle in degrees
FORCE_FUSION_ROLL_MAX=40

# Minimum roll angle in degrees
FORCE_FUSION_ROLL_MIN=-40

# Color for roll indicator pointer (cyan)
FORCE_FUSION_ROLL_POINTER_COLOR="#00ffff"

# Color for speed display (green)
FORCE_FUSION_SPEED_COLOR="#4CAF50"

# Maximum speed value in display units (likely mph or km/h)
FORCE_FUSION_SPEED_MAX=60

# Minimum speed value in display units
FORCE_FUSION_SPEED_MIN=0

# Speed update interval in milliseconds
FORCE_FUSION_SPEED_UPDATE_INTERVAL=100

# Color used for success indicators (green)
FORCE_FUSION_SUCCESS_COLOR="#2ecc71"

# Default text color (off-white)
FORCE_FUSION_TEXT_COLOR="#eeeeee"

# Standard map tile size in pixels
FORCE_FUSION_TILE_SIZE=256

# Color for high tire force values (red)
FORCE_FUSION_TIRE_FORCE_COLOR_HIGH="#e74c3c"

# Color for lime tier tire force values (bright lime)
FORCE_FUSION_TIRE_FORCE_COLOR_LIME="#b4ff32"

# Color for low tire force values (green)
FORCE_FUSION_TIRE_FORCE_COLOR_LOW="#2ecc71"

# Color for normal tire force values (yellow)
FORCE_FUSION_TIRE_FORCE_COLOR_NORMAL="#f1c40f"

# Color for orange tier tire force values (orange)
FORCE_FUSION_TIRE_FORCE_COLOR_ORANGE="#ff7800"

# Maximum tire force value in Newtons
FORCE_FUSION_TIRE_FORCE_MAX=2500

# Minimum tire force value in Newtons
FORCE_FUSION_TIRE_FORCE_MIN=0

# Normal/reference tire force value in Newtons
FORCE_FUSION_TIRE_FORCE_NORMAL=2500

# Update interval in milliseconds for tire force display
FORCE_FUSION_TIRE_FORCE_UPDATE_INTERVAL=100

# Width of trajectory lines on maps in pixels
FORCE_FUSION_TRAJECTORY_LINE_WIDTH=2

# Color used for warning indicators (yellow)
FORCE_FUSION_WARNING_COLOR="#f1c40f"

# Hostname for WebSocket server
FORCE_FUSION_WS_HOST="localhost"

# Port for WebSocket server
FORCE_FUSION_WS_PORT=8765

# Milliseconds between reconnection attempts for WebSocket
FORCE_FUSION_WS_RECONNECT_INTERVAL=1000

# Full WebSocket URI (usually ws://hostname:port)
FORCE_FUSION_WS_URI="ws://localhost:8765"

